			<SymbolDefinition X="0.0" Y="0.0" DefaultBlockName="NO-BN-2D-JBTFE-ANNOTATIONS-CHAINBREAK-Schematic" >
				<SymbolAttribute Tag="FRA_KM" DataType="Int32" >
					<Value>{% assign boolsk = {{alignmentRotation | minus:currentView | cosisnegative}} %}{% if boolsk == 'True' %}{{Mileage | divided_by:1000 | toint}}{% else %}{{MileageIn | divided_by:1000 | toint}}{% endif %}</Value>
				</SymbolAttribute>
				<SymbolAttribute Tag="TIL_KM" DataType="Int32" >
					<Value>{% assign boolsk = {{alignmentRotation | minus:currentView | cosisnegative}} %}{% if boolsk == 'True' %}{{MileageIn | divided_by:1000 | toint}}{% else %}{{Mileage | divided_by:1000 | toint}}{% endif %}</Value>
				</SymbolAttribute>
				<SymbolAttribute Tag="FRA_M" DataType="Double" >
					<Value>{% assign boolsk = {{alignmentRotation | minus:currentView | cosisnegative}} %}{% if boolsk == 'True' %}{{Mileage | modulo:1000}}{% else %}{{MileageIn | modulo:1000}}{% endif %}</Value>
				</SymbolAttribute>
				<SymbolAttribute Tag="TIL_M" DataType="Double" >
					<Value>{% assign boolsk = {{alignmentRotation | minus:currentView | cosisnegative}} %}{% if boolsk == 'True' %}{{MileageIn | modulo:1000}}{% else %}{{Mileage | modulo:1000}}{% endif %}</Value>
				</SymbolAttribute>
				<SymbolAttribute Tag="SPRANG" DataType="Double" >
