<!--========================================================================================================
	Extended data property sets (will be picked up by AutoCAD as Extended Data, and exports to IFC using Civil 3D)
=========================================================================================================-->

<!-- «Tittelfelt» skal inneholde den samme informasjonen som tittelfelt på tegning, i tillegg vil objektstatus  -->
<!-- og revisjonsinformasjon ligge på objektnivå og ikke modellnivå/tegningsnivå. -->

<PropertySets>
	<ExtendedDataPropertySet Name="Tittelfelt" IsVisible="true" IsWritable="true" IsLocked="false" >
		<Properties>
			<ExtendedDataProperty Name="Prosjekt" DataType="Text" Description="" DefaultValue="---" IsVisible="true" DisplayOrder="1" />
			<ExtendedDataProperty Name="Parsell" DataType="Text" Description="Koden identifiserer prosjektets områdeinndeling." DefaultValue="---" IsVisible="true" DisplayOrder="2" />
			<ExtendedDataProperty Name="Revisjon" DataType="Text" Description="«Revisjon» vil alltid følge revisjonsnummer på modellnivå. Bokstav avhenger av leveransen. Dersom en modell har utgått benyttes bokstaven U." DefaultValue="---" IsVisible="true" DisplayOrder="3" />
			<ExtendedDataProperty Name="Dato" DataType="Text" Description="Dato for når modellen ble sendt ut med ny revisjon" DefaultValue="---" IsVisible="true" DisplayOrder="4" />
			<ExtendedDataProperty Name="Revisjonen gjelder" DataType="Text" Description="Beskrivende tekst for hva den siste revisjonen inneholder." DefaultValue="---" IsVisible="true" DisplayOrder="5" />
			<ExtendedDataProperty Name="Sist endret i revisjon" DataType="Text" Description="«Sist endret i revisjon» vil gjenspeile siste revisjon hvor det spesifikke objektet hadde endringer." DefaultValue="---" IsVisible="true" DisplayOrder="6" />
			<ExtendedDataProperty Name="Utarbeidet av" DataType="Text" Description="Egenkontroll" DefaultValue="---" IsVisible="true" DisplayOrder="7" />
			<ExtendedDataProperty Name="Kontrollert av" DataType="Text" Description="Fagkontroll" DefaultValue="---" IsVisible="true" DisplayOrder="8" />
			<ExtendedDataProperty Name="Godkjent av" DataType="Text" Description="Oppdragsleder" DefaultValue="---" IsVisible="true" DisplayOrder="9" />
			<ExtendedDataProperty Name="" DataType="Text" Description="" DefaultValue="---" IsVisible="true" DisplayOrder="9" />

		</Properties>
	</ExtendedDataPropertySet>
</PropertySets>



<!--========================================================================================================
    ObjectType element 'CustomAttribute' macros for Tittelfelt
=========================================================================================================-->


 
 <!-- RailCOMPLETE benytter en extended data definisjon (element PropertySets / ExtendedDataPropertySet) for å definere et  -->
 <!-- property set som kan legges inn i objekters AutoCAD Extended Data. De dukker da opp i alle Autodesk produkter som  -->
 <!-- objektdata / Extended Data. -->
 
 <!-- Data som ligger som Extended Data i et AutoCAD-objekt vil "bli med på lasset" over til IFC dersom du har Civil 3D installert -->
 <!-- og benytter kommandoen for eksport til IFC, EXPORTIFC. -->

	<!-- <xpp:expand select="NOBN_com_PSET_TITTELFELT" /> -->
<xpp:define name="NOBN_com_PSET_TITTELFELT" >
	<xpp:bloc>
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Prosjekt" Name="TittelfeltProsjekt" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Prosjekt" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Parsell" Name="TittelfeltParsell" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Parsell" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Revisjon" Name="TittelfeltRevisjon" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Revisjon" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Dato" Name="TittelfeltDato" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Dato" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Revisjonen gjelder" Name="TittelfeltRevisjonenGjelder" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Revisjonen gjelder" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Sist endret i revisjon" Name="TittelfeltSistEndretIRevisjon" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Sist endret i revisjon" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Utarbeidet av" Name="TittelfeltUtarbeidetAv" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Utarbeidet av" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Kontrollert av" Name="TittelfeltKontrollertAv" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Kontrollert av" />
				
		<CustomAttribute Category="Pset Tittelfelt" DataType="String" DisplayName="Pset Tittelfelt Godkjent av" Name="TittelfeltGodkjentAv" DefaultValue="" 
				ExtendedDataPropertySet="Tittelfelt" ExtendedDataPropertyDefinition="Godkjent av" />
	</xpp:bloc>
</xpp:define>



