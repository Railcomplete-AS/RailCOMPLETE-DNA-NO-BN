<!--========================================================================================================
	Extended data property sets (will be picked up by AutoCAD as Extended Data, and exports to IFC using Civil 3D)
=========================================================================================================-->

<!-- «BNmodellinformasjon» skal inneholde den samme informasjonen som BNmodellinformasjon på tegning, i tillegg vil objektstatus  -->
<!-- og revisjonsinformasjon ligge på objektnivå og ikke modellnivå/tegningsnivå. -->

<!-- Oppdatert for Multiconsult -->

<PropertySets>
	<ExtendedDataPropertySet Name="BNmodellinformasjon" IsVisible="true" IsWritable="true" IsLocked="false" >
		<Properties>
			<ExtendedDataProperty Name="Revisjon" DataType="Text" Description="Angivelse av første revisjon 00C ved MMI400. Deretter følges revisjonsnummerering." DefaultValue="---" IsVisible="true" DisplayOrder="1" />
			<ExtendedDataProperty Name="Revisjonsmerknad" DataType="Text" Description="Kommentarer for endringen i gjeldende revisjon" DefaultValue="---" IsVisible="true" DisplayOrder="2" />
			<ExtendedDataProperty Name="Dato" DataType="Text" Description="Revisjonsdato" DefaultValue="---" IsVisible="true" DisplayOrder="3" />
			<ExtendedDataProperty Name="UtarbeidetAv" DataType="Text" Description="Person som har utført modellering" DefaultValue="---" IsVisible="true" DisplayOrder="4" />
			<ExtendedDataProperty Name="KontrollertAv" DataType="Text" Description="Person som har kontrollert fagmodell" DefaultValue="---" IsVisible="true" DisplayOrder="5" />	
			<ExtendedDataProperty Name="GodkjentAv" DataType="Text" Description="Person som har godkjent fagmodell" DefaultValue="---" IsVisible="true" DisplayOrder="6" />			
			<ExtendedDataProperty Name="FaseBygges" DataType="Text" Description="Fasen der objektet starte byggingen" DefaultValue="---" IsVisible="true" DisplayOrder="7" />
			<ExtendedDataProperty Name="FaseRives" DataType="Text" Description="Fasen der objektet starte riving" DefaultValue="---" IsVisible="true" DisplayOrder="8" />
		</Properties>
	</ExtendedDataPropertySet>
</PropertySets>



<!--========================================================================================================
    ObjectType element 'CustomAttribute' macros for BNmodellinformasjon
=========================================================================================================-->


 
 <!-- RailCOMPLETE benytter en extended data definisjon (element PropertySets / ExtendedDataPropertySet) for å definere et  -->
 <!-- property set som kan legges inn i objekters AutoCAD Extended Data. De dukker da opp i alle Autodesk produkter som  -->
 <!-- objektdata / Extended Data. -->
 
 <!-- Data som ligger som Extended Data i et AutoCAD-objekt vil "bli med på lasset" over til IFC dersom du har Civil 3D installert -->
 <!-- og benytter kommandoen for eksport til IFC, EXPORTIFC. -->

	<!-- <xpp:expand select="NOBN_com_PSET_TITTELFELT" /> -->
<xpp:define name="NOBN_com_PSET_TITTELFELT" >
	<xpp:bloc>
	
		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="Revisjon" Name="BNmodellinformasjonRevisjon" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="Revisjon" />

		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="Revisjonsmerknad" Name="BNmodellinformasjonRevisjonsmerknad" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="Revisjonsmerknad" />
				
		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="Dato" Name="BNmodellinformasjonDato" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="Dato" />
		
		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="UtarbeidetAv" Name="BNmodellinformasjonUtarbeidetAv" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="UtarbeidetAv" />
				
		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="KontrollertAv" Name="BNmodellinformasjonKontrollertAv" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="KontrollertAv" />		

		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="BNmodellinformasjon GodkjentAv" Name="BNmodellinformasjonGodkjentAv" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="GodkjentAv" />				
				
		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="FaseBygges" Name="BNmodellinformasjonFaseBygges" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="FaseBygges" />

		<CustomAttribute Category="BNmodellinformasjon" DataType="String" DisplayName="FaseRives" Name="BNmodellinformasjonFaseRives" DefaultValue="" 
				ExtendedDataPropertySet="BNmodellinformasjon" ExtendedDataPropertyDefinition="FaseRives" />					

	</xpp:bloc>
</xpp:define>



