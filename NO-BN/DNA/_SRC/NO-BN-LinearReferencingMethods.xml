<PositionToolSettings Name="SNCF1" LuaKmFormattingFunctionName="SNCF_placeholder1"/>
<PositionToolSettings Name="SNCF2 LuaKmFormattingFunctionName="SNCF_placeholder2"/>

<LuaFunction Name="SNCF_placeholder1()" ReturnType="String" Description="kake" HideFromUser="true">
	<Constructor>Double SNCF_placeholder1()</Constructor>
	<Formula>
		function SNCF_placeholder1(x)
			return "1-"..math.floor(x).."+"..math.floor(x%1*10^6+0.5)/10^3
		end
	</Formula>
</LuaFunction>

<LuaFunction Name="SNCF_placeholder2()" ReturnType="String" Description="kake" HideFromUser="true">
