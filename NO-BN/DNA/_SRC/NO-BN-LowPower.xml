				<SetValue Key="EarthingMethod" Value="Rail" />
				<SetValue Key="dir" Value="up" />
				<SetValue Key="Model3dName" Value="NO-BN-3D-EL-LYA-LYSARMATUR-INNENDØRS-LYSSTOFFRØR-1200x2" />
