			<Variant Name="Øvrige" >
				<SetValue Key="SubVariant" Value="Ukjent" />
				<SetValue Key="PoleRouting" Value="None" />
				<SetValue Key="EarthingMethod" Value="Unknown" />
			</Variant>
		</Variants>

