(defun Tounge (blockName toungePos side toungeLen / )
	(setvar "ATTDIA" 0)
	(setvar "ATTREQ" 0)
    (command
		"._COLOR" "ByBlock"
		"._LAYER" "Set" "0" ""
		"._LINE" toungePos (strcat "@" (rtos toungeLen) ",0") ""
		"._LAYER" "Set" "0" ""
	)
	(newBlock (strcat blockName "-" side))
