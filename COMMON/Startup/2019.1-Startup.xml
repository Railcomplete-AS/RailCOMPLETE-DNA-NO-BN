<?xml version="1.0" encoding="utf-8"?>
<Startup>

	<!-- ************************************************************************************************ -->

	<!-- This is a startup configuration file for RailCOMPLETE. UIt has XML syntax, but has extension '.rc' -->
	<!-- in order to protect the file a little from curious users. -->
	<!-- This file's contents MUST be transfered to the RC.bundle folder and named 'startup.rc' (case  -->
	<!-- insensitive filename), otherwise RailCOMPLETE won't find startup.rc and will not load properly. -->
	<!-- A typical message to the user will be: -->
	<!-- "License test failed for RailCOMPLETE module RailCOMPLETE. Module will not be loaded." -->
	<!-- The same will happen if there is a syntax error in the startup.rc file. -->
 
	<!-- A typical valid configuration is this one:  -->
	
	<!-- Relative to RC.bundle folder (things that don't need a RC-START'ed document):   -->
	<!-- <GeneralFaqPath>.\FAQ\general-faq.xml</GeneralFaqPath> -->
	<!-- <GeneralWebPagesPath>.\WebLinks\general-WebLinks.xml</GeneralWebPagesPath> -->
	<!-- <AdministrationsFolder>.\Adm</AdministrationsFolder> -->
	<!-- <TutorialsFolder>.\Tutorials</TutorialsFolder> -->

	<!-- Relative to AdministrationFolder (once you have opened a RC-START'ed document): -->
	<!-- <DnaFolder>.\DNA</DnaFolder> -->
	<!-- <ReleaseNotesFolder>.\Help\ReleaseNotes</ReleaseNotesFolder> -->
	<!-- <SoundResourcesFolder>.\Resources\Sounds</SoundResourcesFolder> -->
	<!-- <ButtonImagesFolder>.\ButtonImages</ButtonImagesFolder> -->

	<!-- ************************************************************************************************ -->

	<!-- Relative to RC.bundle folder (things that don't need a RC-START'ed document):   -->
	<GeneralFaqPath>.\FAQ\general-faq.xml</GeneralFaqPath>
	<GeneralWebPagesPath>.\WebLinks\general-WebLinks.xml</GeneralWebPagesPath>
	<AdministrationsFolder>.\Adm</AdministrationsFolder>
	<TutorialsFolder>.\Tutorials</TutorialsFolder>
	<ReleaseNotesFolder>.\Help\ReleaseNotes</ReleaseNotesFolder>
	<SoundResourcesFolder>.\Resources\Sounds</SoundResourcesFolder>
	<ButtonImagesFolder>.\ButtonImages</ButtonImagesFolder>
	<DefaultManageObjectColumnSettingsPath>.\ManageObjectsSettings\DefaultColumnSettings.xml</DefaultManageObjectColumnSettingsPath>
	<LuaSyntaxHighlightingPath>.\Resources\Lua\Highlighting\Lua.xshd</LuaSyntaxHighlightingPath>
	<LuaTooltipSyntaxHighlightingPath>.\Resources\Lua\Highlighting\Tooltip.xshd</LuaTooltipSyntaxHighlightingPath>
|
	<!-- NB! NB! NB! Relative to *AdministrationFolder* (once you have opened a RC-START'ed document): -->
	<!-- RailCOMPLETE, once you launch the RC-START command, looks for subfolder names in the folder given by the 'DnaFolder' element. -->
	<!-- For instance, if DnaFolder has been set to ".\DNA", then RailCOMPLETE will search RC.bundle\Adm. If if finds, as an example, -->
	<!-- the sub folders 'RC.bundle\Adm\XX-GL' and 'RC.bundle\Adm\NO-BN', then TWO administrations will be offered: XX-GL and NO-BN. -->
	<DnaFolder>.\DNA</DnaFolder>
</Startup>